{
    "displayName": "Unigraph Core",
    "version": "0.0.1",
    "description": "The core package of Unigraph, contains many high level abstractions for development.",
    "unigraph": {
        "package_name": "unigraph.core",
        "schemas": [{
            "id": "list",
            "src": "schemas/list.json"
        }, {
            "id": "view",
            "src": "schemas/view.json"
        }, {
            "id": "context_menu_item",
            "src": "schemas/context_menu_item.json"
        }, {
            "id": "pinboard",
            "src": "schemas/pinboard.json"
        }],
        "executables": [{
            "id": "add-item-to-list",
            "env": "routine/js",
            "src": "executables/addItemToList.js",
            "editable": true,
            "name": "Add an item to a list object"
        }, {
            "id": "add-item-to-pinboard",
            "env": "routine/js",
            "src": "executables/addItemToPinboard.js",
            "editable": true,
            "name": "Add an item to a pinboard object"
        }, {
            "id": "update-pinboard-layout",
            "env": "routine/js",
            "src": "executables/updatePinboardLayout.js",
            "editable": true,
            "name": "Update a pinboard's layout"
        }, {
            "id": "delete-item-from-list",
            "env": "routine/js",
            "src": "executables/deleteItemFromList.js",
            "editable": true,
            "name": "Deletes an item from a list object"
        }]
    }
}