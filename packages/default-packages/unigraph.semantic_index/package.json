{
    "displayName": "Semantic indexing for Unigraph",
    "version": "0.2.5",
    "description": "Semantic indexing for Unigraph",
    "name": "unigraph.semantic_index",
    "unigraph": {
        "entities": [],
        "executables": [
            {
                "name": "Build semantic index for entities",
                "env": "routine/js",
                "id": "build-semantic-index",
                "src": "executables/semanticIndex.js",
                "editable": true,
                "on_hook": "after_object_updated/all"
            },
            {
                "name": "Do semantic search",
                "env": "routine/js",
                "id": "do-semantic-search",
                "src": "executables/doSemanticSearch.js",
                "editable": true
            },
            {
                "name": "Index notes",
                "env": "routine/js",
                "id": "index-notes",
                "src": "executables/indexNotes.js",
                "periodic": "*/1 * * * *",
                "editable": true,
                "concurrency": 1
            },
            {
                "name": "Semantic searcher",
                "env": "component/react-jsx",
                "id": "semantic-searcher-component",
                "src": "executables/semanticSearcher.jsx",
                "editable": true,
                "imports": [
                    {
                        "env": "npm",
                        "package": "@mui/material",
                        "import": "TextField",
                        "import_as": "TextField"
                    },
                    {
                        "env": "npm",
                        "package": "@mui/material",
                        "import": "Button",
                        "import_as": "Button"
                    },
                    {
                        "env": "npm",
                        "package": "@mui/material",
                        "import": "Divider",
                        "import_as": "Divider"
                    },
                    {
                        "env": "npm",
                        "package": "@mui/material",
                        "import": "Card",
                        "import_as": "Card"
                    }
                ]
            },
            {
                "name": "Similarity searcher",
                "env": "component/react-jsx",
                "id": "similarity-searcher-component",
                "src": "executables/similaritySearcher.jsx",
                "editable": true,
                "imports": [
                    {
                        "env": "npm",
                        "package": "@mui/material",
                        "import": "TextField",
                        "import_as": "TextField"
                    },
                    {
                        "env": "npm",
                        "package": "@mui/material",
                        "import": "Button",
                        "import_as": "Button"
                    },
                    {
                        "env": "npm",
                        "package": "@mui/material",
                        "import": "Divider",
                        "import_as": "Divider"
                    },
                    {
                        "env": "npm",
                        "package": "@mui/material",
                        "import": "Card",
                        "import_as": "Card"
                    }
                ]
            }
        ]
    }
}
