{
    "displayName": "RSS Reader",
    "version": "0.0.1",
    "description": "An RSS Reader with Unigraph",
    "unigraph": {
        "package_name": "unigraph.rss_reader",
        "schemas": [{"id": "rss_feed", "src": "schemas/rss_feed.json"}, 
            {"id": "rss_item", "src": "schemas/rss_item.json"}],
        "executables": [{
            "id": "update-feeds",
            "env": "routine/js",
            "src": "executables/updateFeeds.js",
            "periodic": "*/30 * * * *",
            "editable": true,
            "name": "Update feeds every half an hour"
        }, {
            "id": "add-feed",
            "env": "routine/js",
            "src": "executables/addFeed.js",
            "editable": true,
            "name": "Add a feed to RSS feeds list"
        }]
    }
}