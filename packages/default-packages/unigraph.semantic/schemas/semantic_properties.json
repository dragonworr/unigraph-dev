{
    "dgraph.type": "Type",
    "_definition": {
        "type": { "unigraph.id": "$/composer/Object" },
        "_parameters": {
            "_indexedBy": { "unigraph.id": "$/primitive/string" },
            "_indexes": ["name"]
        },
        "_properties": [
            {
                "_key": "children",
                "_definition": {
                    "type": { "unigraph.id": "$/composer/Array" },
                    "_parameters": {
                        "_element": {
                            "type": { "unigraph.id": "$/composer/Union" },
                            "_parameters": {
                                "_definitions": [{
                                    "type": { "unigraph.id": "$/schema/interface/textual" }
                                }, {
                                    "type": { "unigraph.id": "$/schema/interface/semantic" }
                                }, {
                                    "type": { "unigraph.id": "$/schema/tag" }
                                }]
                            }
                        }
                    }
                }
            }
        ]
    }
}