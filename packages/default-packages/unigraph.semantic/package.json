{
    "displayName": "Semantic",
    "version": "0.0.1",
    "description": "Semantic properties support for unigraph objects.",
    "unigraph": {
        "package_name": "unigraph.semantic",
        "schemas": [
            {"id": "color", "src": "schemas/color.json"},
            {"id": "person", "src": "schemas/person.json"},
            {"id": "tag", "src": "schemas/tag.json"},
            {"id": "note", "src": "schemas/note.json"},
            {"id": "interface/textual", "src": "schemas/textual.json"},
            {"id": "interface/semantic", "src": "schemas/semantic.json"},
            {"id": "semantic_properties", "src": "schemas/semantic_properties.json"},
            {"id": "creative_work", "src": "schemas/creative_work.json"},
            {"id": "message", "src": "schemas/message.json"}
        ],
        "executables": [
            { 
                "id": "add-tag-to-object",
                "env": "routine/js",
                "src": "executables/addTagToObject.js",
                "editable": true,
                "name": "Adds a tag to an object"
            }
        ]
    }
}