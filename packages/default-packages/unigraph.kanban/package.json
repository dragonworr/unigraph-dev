{
    "displayName": "Unigraph Kanban",
    "version": "0.2.5",
    "description": "Kanban component for Unigraph",
    "name": "unigraph.kanban",
    "unigraph": {
        "executables": [
            {
                "id": "kanban-view",
                "env": "component/react-jsx",
                "src": "executables/kanbanView.jsx",
                "editable": true,
                "name": "Kanban view",
                "imports": [
                    {
                        "env": "npm",
                        "package": "@mui/material",
                        "import": "Typography",
                        "import_as": "Typography"
                    },
                    {
                        "env": "npm",
                        "package": "@mui/material",
                        "import": "Card",
                        "import_as": "Card"
                    }
                ]
            },
            {
                "id": "kanban-list-view",
                "env": "component/react-jsx",
                "src": "executables/kanbanListView.jsx",
                "editable": true,
                "name": "Kanban list view",
                "imports": [
                    {
                        "env": "npm",
                        "package": "@mui/material",
                        "import": "TextField",
                        "import_as": "TextField"
                    },
                    {
                        "env": "npm",
                        "package": "@mui/material",
                        "import": "Button",
                        "import_as": "Button"
                    }
                ]
            },
            {
                "id": "kanban-view-detailed",
                "env": "component/react-jsx",
                "src": "executables/kanbanViewDetailed.jsx",
                "editable": true,
                "name": "Kanban view detailed",
                "imports": [
                    {
                        "env": "npm",
                        "package": "@mui/material",
                        "import": "Typography",
                        "import_as": "Typography"
                    },
                    {
                        "env": "npm",
                        "package": "@mui/material",
                        "import": "Card",
                        "import_as": "Card"
                    },
                    {
                        "env": "npm",
                        "package": "@mui/material",
                        "import": "TextField",
                        "import_as": "TextField"
                    },
                    {
                        "env": "npm",
                        "package": "@mui/material",
                        "import": "Button",
                        "import_as": "Button"
                    },
                    {
                        "env": "npm",
                        "package": "@mui/material",
                        "import": "MenuItem",
                        "import_as": "MenuItem"
                    },
                    {
                        "env": "npm",
                        "package": "@mui/material/icons",
                        "import": "MoreVert",
                        "import_as": "MoreVert"
                    }
                ]
            }
        ],
        "schemas": [
            {
                "id": "kanban",
                "src": "schemas/kanban.json"
            }
        ],
        "entities": [
            {
                "id": "kanban_view",
                "src": "entities/kanban_view.json"
            },
            {
                "id": "kanban_list_view",
                "src": "entities/kanban_list_view.json"
            },
            {
                "id": "kanban_view_detailed",
                "src": "entities/kanban_view_detailed.json"
            }
        ]
    }
}
